{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:recommended"
    ],
    "lockFileMaintenance": {
        "enabled": true
    },
    "packageRules": [
        {
            "groupName": "pre-commit",
            "matchManagers": [
                "pre-commit"
            ]
        }
    ],
    "pre-commit": {
        "enabled": true
    }
}
